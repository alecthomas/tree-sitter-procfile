{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "procfile",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "process_definition"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "process_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "declaration"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "execution"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "multiline_block"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "process_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_declaration_item"
          }
        }
      ]
    },
    "process_name": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "!"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_declaration_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "option"
        },
        {
          "type": "SYMBOL",
          "name": "glob_pattern"
        },
        {
          "type": "SYMBOL",
          "name": "exclusion_pattern"
        }
      ]
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "option_key"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "option_value"
          }
        }
      ]
    },
    "option_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_bare_option_value"
        }
      ]
    },
    "_bare_option_value": {
      "type": "PATTERN",
      "value": "[^\\s:]+"
    },
    "glob_pattern": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "bare_glob"
        }
      ]
    },
    "exclusion_pattern": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_quoted_string"
            },
            {
              "type": "SYMBOL",
              "name": "bare_glob"
            }
          ]
        }
      ]
    },
    "execution": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "env_var"
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "command"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "command"
        }
      ]
    },
    "env_var": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "env_key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "env_value"
        }
      ]
    },
    "env_key": {
      "type": "PATTERN",
      "value": "[A-Z_][A-Z0-9_]*"
    },
    "env_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_bare_env_value"
        }
      ]
    },
    "_bare_env_value": {
      "type": "PATTERN",
      "value": "[^\\s]+"
    },
    "command": {
      "type": "SYMBOL",
      "name": "_command_text"
    },
    "multiline_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "block_line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    },
    "block_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_multiline_command_text"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_quoted_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        }
      ]
    },
    "single_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "PATTERN",
          "value": "[^']*"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "double_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\"\\\\]+"
              },
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\."
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": " +"
    },
    {
      "type": "SYMBOL",
      "name": "line_continuation"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_newline"
    },
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    },
    {
      "type": "SYMBOL",
      "name": "_command_text"
    },
    {
      "type": "SYMBOL",
      "name": "_multiline_command_text"
    },
    {
      "type": "SYMBOL",
      "name": "line_continuation"
    },
    {
      "type": "SYMBOL",
      "name": "option_key"
    },
    {
      "type": "SYMBOL",
      "name": "bare_glob"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}